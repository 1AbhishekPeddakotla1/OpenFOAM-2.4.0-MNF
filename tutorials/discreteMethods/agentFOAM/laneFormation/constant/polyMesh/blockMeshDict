FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;//20.588

vertices
(
    (0 0 -0.05) 
    (100 0 -0.05)
    (100 100 -0.05)
    (0 100 -0.05)
    (0 0 0.05)
    (100 0 0.05)
    (100 100 0.05)
    (0 100 0.05)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) zone (10 10 1) simpleGrading (1 1 1)
);

boundary
(
    sides
    {
        type wall;
        faces ((1 2 6 5) (0 4 7 3) (2 3 7 6) (0 1 5 4));
    }

    topBottom
    {
        type empty;
      	faces ((4 5 6 7) (0 3 2 1));
    }
)

mergePatchPairs
(
);


