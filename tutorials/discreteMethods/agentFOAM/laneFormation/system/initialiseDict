FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          agentInitialiseDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


agentConfigurations
(
    configuration
    {
        model                    randomInitialiseBox;
        
        randomInitialiseBoxProperties
        {
            agentId                 A;
            N                       300;
            mass                    70;
            temperature             1e23;
            binWidth                0.01;
            boundBox
            {
                startPoint          (0 0 0);
                endPoint            (20 100 0);
            }
        }
    }
    
    configuration
    {
        model                    randomInitialiseBox;
        
        randomInitialiseBoxProperties
        {
            agentId                 B;
            N                       300;
            mass                    70;
            temperature             1e23;
            binWidth                0.01;
            boundBox
            {
                startPoint          (80 0 0);
                endPoint            (100 100 0);
            }
        }
    }    
 
);





