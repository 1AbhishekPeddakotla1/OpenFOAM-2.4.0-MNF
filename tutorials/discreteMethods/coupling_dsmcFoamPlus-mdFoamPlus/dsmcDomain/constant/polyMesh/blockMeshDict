FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/polyMesh";
    object      blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 5e-8;

// DSMC domain size in nm (250 50 50) - 50nm gap in middle for MD domain

vertices
(
    //Vertices for left DSMC zone
    (0 0 0) //0
    (1.908 0 0) //1
    (1.908 1 0) //2
    (0 1 0) //3
    (0 0 1) //4
    (1.908 0 1) //5
    (1.908 1 1) //6
    (0 1 1) //7

    //Vertices for left coupling zone 
    (1.908 0 0) //8
    (2 0 0) //9
    (2 1 0) //10
    (1.908 1 0) //11
    (1.908 0 1) //12
    (2 0 1) //13
    (2 1 1) //14
    (1.908 1 1) //15

    //Vertices for right coupling zone
    (3 0 0) //16
    (3.092 0 0) //17
    (3.092 1 0) //18
    (3 1 0) //19
    (3 0 1) //20
    (3.092 0 1) //21
    (3.092 1 1) //22
    (3 1 1) //23

    //Vertices for right DSMC zone
    (3.092 0 0) //24
    (5 0 0) //25
    (5 1 0) //26
    (3.092 1 0) //27
    (3.092 0 1) //28
    (5 0 1) //29
    (5 1 1) //30
    (3.092 1 1) //31    
);

blocks
(
    hex (0 1 2 3 4 5 6 7) dsmcZoneLeft (16 1 1) simpleGrading (1 1 1)
    hex (8 9 10 11 12 13 14 15) couplingZoneLeft (1 1 1) simpleGrading (1 1 1)
    hex (16 17 18 19 20 21 22 23) couplingZoneRight (1 1 1) simpleGrading (1 1 1)
    hex (24 25 26 27 28 29 30 31) dsmcZoneRight (16 1 1) simpleGrading (1 1 1)
);

boundary
(
    //dsmcZoneLeft boundaries
    cyclicX_left
    {
        type cyclic;
        faces ((7 3 0 4));
        neighbourPatch cyclicX_right;
    }     

    cyclicY_left_top_main
    {
        type cyclic;
        faces ((7 6 2 3));
        neighbourPatch cyclicY_left_bottom_main;
    }

    cyclicY_left_bottom_main
    {
        type cyclic;
        faces ((4 5 1 0));
        neighbourPatch cyclicY_left_top_main;
    }  

    cyclicZ_left_front_main
    {
        type cyclic;
        faces ((3 2 1 0));
        neighbourPatch cyclicZ_left_back_main;
    }

    cyclicZ_left_back_main
    {
        type cyclic;
        faces ((7 6 5 4));
        neighbourPatch cyclicZ_left_front_main;
    }

    //dsmcCouplingZoneLeft boundaries
    couplingX_middle_left
    {
        type patch;
        faces ((14 10 9 13));
    }

    cyclicY_left_top_coupled
    {
        type cyclic;
        faces ((15 14 10 11));
        neighbourPatch cyclicY_left_bottom_coupled;
    }

    cyclicY_left_bottom_coupled
    {
        type cyclic;
        faces ((12 13 9 8));
        neighbourPatch cyclicY_left_top_coupled;
    }

    cyclicZ_left_front_coupled
    {
        type cyclic;
        faces ((11 10 9 8));
        neighbourPatch cyclicZ_left_back_coupled;
    }

    cyclicZ_left_back_coupled
    {
        type cyclic;
        faces ((15 14 13 12));
        neighbourPatch cyclicZ_left_front_coupled;
    }   

    //dsmcCouplingZoneRight boundaries
    couplingX_middle_right
    {
        type patch;
        faces ((23 19 16 20));
    }

    cyclicY_right_top_coupled
    {
        type cyclic;
        faces ((23 22 18 19));
        neighbourPatch cyclicY_right_bottom_coupled;
    }

    cyclicY_right_bottom_coupled
    {
        type cyclic;
        faces ((20 21 17 16));
        neighbourPatch cyclicY_right_top_coupled;
    }
    
    cyclicZ_right_front_coupled
    {
        type cyclic;
        faces ((19 18 17 16));
        neighbourPatch cyclicZ_right_back_coupled;
    }

    cyclicZ_right_back_coupled
    {
        type cyclic;
        faces ((23 22 21 20));
        neighbourPatch cyclicZ_right_front_coupled;
    }  
    
    //dsmcZoneRight boundaries
    cyclicX_right
    {
        type cyclic;
        faces ((30 26 25 29));
        neighbourPatch cyclicX_left;
    }

    cyclicY_right_top_main
    {
        type cyclic;
        faces ((31 30 26 27));
        neighbourPatch cyclicY_right_bottom_main;
    }

    cyclicY_right_bottom_main
    {
        type cyclic;
        faces ((28 29 25 24));
        neighbourPatch cyclicY_right_top_main;
    }  

    cyclicZ_right_front_main
    {
        type cyclic;
        faces ((27 26 25 24));
        neighbourPatch cyclicZ_right_back_main;
    }

    cyclicZ_right_back_main
    {
        type cyclic;
        faces ((31 30 29 28));
        neighbourPatch cyclicZ_right_front_main;
    }  
)

mergePatchPairs
(
);


