FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/polyMesh";
    object      blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 5e-8;

// DSMC domain size in nm (250 50 50) - 50nm gap in middle for MD domain

vertices
(
    //Vertices for left DSMC zone
    (0 0 0) //0
    (1.9 0 0) //1
    (1.9 1 0) //2
    (0 1 0) //3
    (0 0 1) //4
    (1.9 0 1) //5
    (1.9 1 1) //6
    (0 1 1) //7
    (2 0 0) //8
    (2 1 0) //9
    (2 0 1) //10
    (2 1 1) //11

    //Vertices for right DSMC zone    
    (3 0 0) //12
    (3.1 0 0) //13
    (3.1 1 0) //14
    (3 1 0) //15
    (3 0 1) //16
    (3.1 0 1) //17
    (3.1 1 1) //18
    (3 1 1) //19   
    (5 0 0) //20  
    (5 1 0) //21  
    (5 0 1) //22  
    (5 1 1) //23  
);

blocks
(
    hex (0 1 2 3 4 5 6 7) dsmcZoneLeft (19 1 1) simpleGrading (1 1 1)
    hex (1 8 9 2 5 10 11 6) couplingZoneLeft (1 1 1) simpleGrading (1 1 1)
    hex (12 13 14 15 16 17 18 19) couplingZoneRight (1 1 1) simpleGrading (1 1 1) 
    hex (13 20 21 14 17 22 23 18) dsmcZoneRight (19 1 1) simpleGrading (1 1 1)
);

boundary
(
    //Left boundaries
    cyclicX_left
    {
        type cyclic;
        faces ((0 3 7 4));
        neighbourPatch cyclicX_right;
    }

    couplingX_left_middle
    {
        type patch;
        faces ((8 9 11 10));
    }   

    cyclicY_left_top
    {
        type cyclic;
        faces ((3 2 6 7));
        neighbourPatch cyclicY_left_bottom;
    }

    cyclicY_left_bottom
    {
        type cyclic;
        faces ((0 1 5 4));
        neighbourPatch cyclicY_left_top;
    }

    cyclicY_left_coupling_top
    {
        type cyclic;
        faces ((2 9 11 6));
        neighbourPatch cyclicY_left_coupling_bottom;
    }

    cyclicY_left_coupling_bottom
    {
        type cyclic;
        faces ((1 8 10 5));
        neighbourPatch cyclicY_left_coupling_top;
    } 

    cyclicZ_left_front
    {
        type cyclic;
        faces ((0 1 2 3));
        neighbourPatch cyclicZ_left_back;
    }

    cyclicZ_left_back
    {
        type cyclic;
        faces ((4 5 6 7));
        neighbourPatch cyclicZ_left_front;
    }

    cyclicZ_left_coupling_front
    {
        type cyclic;
        faces ((1 8 9 2));
        neighbourPatch cyclicZ_left_coupling_back;
    }

    cyclicZ_left_coupling_back
    {
        type cyclic;
        faces ((5 10 11 6));
        neighbourPatch cyclicZ_left_coupling_front;
    }

    //Right boundaries
    cyclicX_right
    {
        type cyclic;
        faces ((20 21 23 22));
        neighbourPatch cyclicX_left;
    }

    couplingX_right_middle
    {
        type patch;
        faces ((12 15 19 16));
    }

    cyclicY_right_coupling_top
    {
        type cyclic;
        faces ((15 14 18 19));
        neighbourPatch cyclicY_right_coupling_bottom;
    }

    cyclicY_right_coupling_bottom
    {
        type cyclic;
        faces ((12 13 17 16));
        neighbourPatch cyclicY_right_coupling_top;
    }  

    cyclicY_right_top
    {
        type cyclic;
        faces ((14 21 23 18));
        neighbourPatch cyclicY_right_bottom;
    }

    cyclicY_right_bottom
    {
        type cyclic;
        faces ((13 20 22 17));
        neighbourPatch cyclicY_right_top;
    }

    cyclicZ_right_coupling_front
    {
        type cyclic;
        faces ((12 13 14 15));
        neighbourPatch cyclicZ_right_coupling_back;
    }

    cyclicZ_right_coupling_back
    {
        type cyclic;
        faces ((16 17 18 19));
        neighbourPatch cyclicZ_right_coupling_front;
    } 

    cyclicZ_right_front
    {
        type cyclic;
        faces ((13 20 21 14));
        neighbourPatch cyclicZ_right_back;
    }

    cyclicZ_right_back
    {
        type cyclic;
        faces ((17 22 23 18));
        neighbourPatch cyclicZ_right_front;
    }
)

mergePatchPairs
(
);


