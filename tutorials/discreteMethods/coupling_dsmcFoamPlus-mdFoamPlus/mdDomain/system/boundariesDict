FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      boundariesDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
polyPatchBoundaries
(
    boundary
    {
        boundaryModel  polyCouplingPatch;
                
        patchBoundaryProperties
        {
            patchName      couplingX_left;
        }

        polyCouplingPatchProperties
        {
		molIds (Ar);
	}
    }
    boundary
    {
        boundaryModel  polyCouplingPatch;
        
        patchBoundaryProperties
        {
            patchName      couplingX_right;
        }

        polyCouplingPatchProperties
        {
		molIds (Ar);
	}
    }    
);

polyCyclicBoundaries
(    
    boundary
    {
        boundaryModel  polyStandardCyclic;
        patchName      periodicY_back;
    }
    boundary
    {
        boundaryModel  polyStandardCyclic;
        patchName      periodicY_front;
    }
    boundary
    {
        boundaryModel  polyStandardCyclic;
        patchName      periodicZ_top;
    }
    boundary
    {
        boundaryModel  polyStandardCyclic;
        patchName      periodicZ_bottom;
    }
);

